# Configuración de Logging
LOG_LEVEL=DEBUG

# Airflow
AIRFLOW_UID=50000

# Token JWT para Grafana (opcional - generarlo con: curl -X POST http://localhost:9095/auth/token)
# AIRFLOW_JWT_TOKEN=your_jwt_token_here

# PostgreSQL (usado por energiafacilities y teleows)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=integratel_db
POSTGRES_USER=postgres
POSTGRES_PASS=your_password_here

# Teleows - Portal Integratel (solo para desarrollo local)
# En producción, usar Airflow Connection "teleows_portal"
TELEOWS_USERNAME=your_username
TELEOWS_PASSWORD=your_password

# Entorno de configuración
TELEOWS_ENV=default
ENV_MODE=dev
