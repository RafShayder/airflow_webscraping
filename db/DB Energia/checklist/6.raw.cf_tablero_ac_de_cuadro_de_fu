
--CF - TABLERO AC DE CUADRO DE FU
CREATE TABLE IF NOT EXISTS raw.cf_tablero_ac_de_cuadro_de_fu (
    task_id                           VARCHAR(255) NOT NULL,
    site_id                           VARCHAR(255) NOT NULL,
    sub_wo_id                         VARCHAR(255) NOT NULL,
    cuadro_fza_que_perten_coloca      VARCHAR(255),
    capacidad_de_itm_principal_a      VARCHAR(255),
    cantid_itm_no_inclui_interr       VARCHAR(255),
    consumo_en_interrupor_generala    VARCHAR(255),
    voltaje_en_interrupor_generala    VARCHAR(255),
    limp_pintad_tabler_corrie         VARCHAR(255),
    limp_ajuste_interr_conexi         VARCHAR(255),
    medici_verif_balanc_carga         VARCHAR(255),
    limpieza_y_calibracion_de_tvss    VARCHAR(255),
    limp_calibr_transf                VARCHAR(255),
    limp_calibr_shunt                 VARCHAR(255),
    limp_calibr_conexi_tierra         VARCHAR(255),
    medici_temper_camara_termog       VARCHAR(255),
    actual_diagra_unifil              VARCHAR(255),
    rotulacion_de_equipos             VARCHAR(255),
    observaciones                     VARCHAR(255)
);
