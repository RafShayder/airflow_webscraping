
CREATE TABLE IF NOT EXISTS raw.ie_suministro_de_energia (
  task_id                           VARCHAR(255) NOT NULL,
  site_id                           VARCHAR(255) NOT NULL,
  sub_wo_id                         VARCHAR(255) NOT NULL,

  fuente_de_energia                 VARCHAR(255),
  compania_electrica                VARCHAR(255),
  pertenencia_de_suministro         VARCHAR(255),
  tipo_de_tension_del_suministro    VARCHAR(255),
  propietario_del_suministro        VARCHAR(255),
  num_ro_de_suministro              VARCHAR(255),
  num_ro_de_medidor                 VARCHAR(255),
  num_ro_de_cliente                 VARCHAR(255),
  cantidad_de_fases                 VARCHAR(255),
  capacidad_de_itm_de_medidor_a     VARCHAR(255),
  calibr_cable_mm2_itm_tab          VARCHAR(255),
  calibr_cable_mm2_itm_conces       VARCHAR(255),
  voltaje_en_itm_v                  VARCHAR(255),
  distan_entre_sumini_estaci_m      VARCHAR(255),
  observacion                       VARCHAR(255)
);
