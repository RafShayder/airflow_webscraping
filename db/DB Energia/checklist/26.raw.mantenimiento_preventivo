-- Esquema: raw
CREATE TABLE IF NOT EXISTS raw.mantenimiento_preventivo (
  task_id                               VARCHAR(255) NOT NULL,
  site_id                               VARCHAR(255) NOT NULL,
  sub_wo_id                             VARCHAR(255) NOT NULL,

  manten_preven_nom_local               VARCHAR(255),
  energi_rectif_cantid_modulo           VARCHAR(255),
  energi_rectif_carga_actual_dc         VARCHAR(255),
  energi_rectif_observ                  VARCHAR(255),
  energi_rectif_detall_observ           VARCHAR(255),

  energi_bat_cuenta_banco_bat           VARCHAR(255),
  energia_baterias_tipo                 VARCHAR(255),
  energi_bat_cantid_bancos              VARCHAR(255),
  energi_bat_cantid_celdas              VARCHAR(255),
  energi_bat_marca_bat                  VARCHAR(255),
  energia_baterias_observaciones        VARCHAR(255),
  energi_bat_detall_observ              VARCHAR(255),

  infra_estado_de_torre                 VARCHAR(255),
  infra_tiene_observaciones             VARCHAR(255),
  infra_detalles_de_observacion         VARCHAR(255),
  infra_sellado_de_caseta_sala          VARCHAR(255),
  infra_tiene_observaciones1            VARCHAR(255),
  infra_detall_observ_1                 VARCHAR(255),
  infra_hermetizado_de_puerta           VARCHAR(255),
  infra_tiene_observaciones2            VARCHAR(255),
  infra_detall_observ_2                 VARCHAR(255),
  infra_filtra_caseta_sala              VARCHAR(255),
  infra_tiene_observaciones3            VARCHAR(255),
  infra_detall_observ_3                 VARCHAR(255),
  infra_cableado_de_alarmas             VARCHAR(255),
  infra_tiene_observaciones4            VARCHAR(255),
  infra_detall_observ_4                 VARCHAR(255),
  infra_sensores_de_caseta_sala         VARCHAR(255),
  infra_tiene_observaciones5            VARCHAR(255),
  infra_detall_observ_5                 VARCHAR(255),
  infra_coubic_otros_operad             VARCHAR(255),
  infra_tiene_observaciones6            VARCHAR(255),
  infra_detall_observ_6                 VARCHAR(255),

  tabler_energi_estado_tabler           VARCHAR(255),
  tabler_energi_tiene_observ            VARCHAR(255),
  tabler_energi_detall_observ           VARCHAR(255),

  grupo_electr_hay_grupo_electr         VARCHAR(255),
  grupo_electr_tiene_observ             VARCHAR(255),
  grupo_electr_detall_observ            VARCHAR(255),

  sistem_puesta_tierra_hay              VARCHAR(255),
  sistem_puesta_tierra_tiene            VARCHAR(255),
  sistem_puesta_tierra_detall           VARCHAR(255),

  gulnr_bbu_rtn                         VARCHAR(255),
  gulnr_estado_peinad_fo_red            VARCHAR(255),
  gulnr_tiene_observaciones             VARCHAR(255),
  gulnr_detalles_de_observacion         VARCHAR(255),

  observ_general_observ                 VARCHAR(255),

  energi_unidad_distri_buen             VARCHAR(255),
  energi_unidad_distri_tiene            VARCHAR(255),
  energi_unidad_distri_detall           VARCHAR(255),

  clima_aire_acondi_cuenta_aires        VARCHAR(255),
  clima_aire_acondicionado_tipo         VARCHAR(255),
  clima_aire_acondi_cantid_aires        VARCHAR(255),
  clima_aire_acondi_marca_aires         VARCHAR(255),
  clima_aire_acondi_observ              VARCHAR(255),
  clima_aire_acondi_detall              VARCHAR(255)
);

