  capacidad_itm_de_banda_base        VARCHAR(255),
  verificar_etiquetado_de_itm        VARCHAR(255),   -- aparece "OK" en origen
  cantidad_de_rectificadores         VARCHAR(255),
  ajuste_de_conectores_de_itm        VARCHAR(255),   -- aparece "OK" en origen
  capacidad_amp                      VARCHAR(255),

