
CREATE TABLE IF NOT EXISTS raw.lmt_lbt_conductores_y_protecc (
  task_id                         VARCHAR(255) NOT NULL,
  site_id                         VARCHAR(255) NOT NULL,
  sub_wo_id                       VARCHAR(255) NOT NULL,

  conductor                       VARCHAR(255),
  tipo_de_conductor               VARCHAR(255),
  tipo_de_instalacion             VARCHAR(255),
  limp_ajuste_equipa_mecani_elec  VARCHAR(255),
  medici_aislam_linea             VARCHAR(255),
  medici_aislam_pararr            VARCHAR(255),
  existen_pararrayos              VARCHAR(255),
  estado_de_pararrayos            VARCHAR(255),
  existen_spat_para_pararrayos    VARCHAR(255),
  pararr_conect_tierra_pararr     VARCHAR(255),
  medici_resist_spat_ohm          VARCHAR(255),
  dms                              VARCHAR(255),
  medici_distan_minima_seguri     VARCHAR(255),
  faja_de_servidumbre             VARCHAR(255),
  empalme_aereo                   VARCHAR(255),
  empalme_subterraneo             VARCHAR(255),
  observacion                     VARCHAR(255)
);

