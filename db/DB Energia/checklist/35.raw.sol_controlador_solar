
CREATE TABLE raw.sol_controlador_solar (
  task_id                         VARCHAR(255) NOT NULL,
  site_id                         VARCHAR(255) NOT NULL,
  sub_wo_id                       VARCHAR(255) NOT NULL,
  cantid_total_contro_solare      VARCHAR(255),
  cantid_total_contro_solare_1    VARCHAR(255),
  tipo_de_controlador_solar       VARCHAR(255),
  marca                           VARCHAR(255),
  modelo                          VARCHAR(255),
  capacidad_en_w                  VARCHAR(255),
  carga_total_de_la_carga_en_adc  VARCHAR(255),
  carga_sumini_panele_adc         VARCHAR(255),
  carga_sumini_bat_solare_adc     VARCHAR(255),
  pinzar_carga_sumini_otra        VARCHAR(255),
  inspec_visual_limp_unidad       VARCHAR(255),
  calibr_alarma_alto_bajo_voltaj  VARCHAR(255),
  calibr_descon_recone            VARCHAR(255),
  verificar_consumo               VARCHAR(255),
  observacion                     VARCHAR(255)
);

