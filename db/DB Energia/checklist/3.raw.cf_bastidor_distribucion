---CF - BASTIDOR DISTRIBUCION
CREATE TABLE IF NOT EXISTS raw.cf_bastidor_distribucion (
    task_id                         VARCHAR(255) NOT NULL,
    site_id                         VARCHAR(255) NOT NULL,
    sub_wo_id                       VARCHAR(255) NOT NULL,
    cuadro_fza_que_perten_coloca    VARCHAR(255),
    numero_unidad_distri            VARCHAR(255),
    limp_ajuste_pintad_bastid       VARCHAR(255),
    verif_fusibl_e_interr           VARCHAR(255),
    verif_cables_barras_interc      VARCHAR(255),
    calculo_de_caida_de_tension     VARCHAR(255),
    corriente_de_llegada_a          VARCHAR(255),
    voltaje_de_llegada_v            VARCHAR(255),
    temper_camara_termog_c          VARCHAR(255),
    limp_calibr_medido_consum       VARCHAR(255),
    comprobacion_de_alarmas         VARCHAR(255),
    actual_diagra_unifil            VARCHAR(255),
    rotula_bastid_distri_enumer     VARCHAR(255),
    observacion                     VARCHAR(255)
);
