
CREATE TABLE IF NOT EXISTS raw.ie_mantenimiento_pozo_por_poz (
  task_id                             VARCHAR(255) NOT NULL,
  site_id                             VARCHAR(255) NOT NULL,
  sub_wo_id                           VARCHAR(255) NOT NULL,

  pozo_interv_perten_una_malla        VARCHAR(255),
  nnum_pozo_a_intervenir              VARCHAR(255),
  nnum_malla_a_intervenir             VARCHAR(255),
  sistema_que_protege                 VARCHAR(255),
  sala_o_equipos_que_protege          VARCHAR(255),
  verif_contin_pozo_otro_pozo         VARCHAR(255),
  verif_contin_pozo_barra_equipo      VARCHAR(255),
  verif_resist_menor_5_ohmios_bt      VARCHAR(255),
  limp_ajuste_conect_engras           VARCHAR(255),
  pintado_y_senalizacion              VARCHAR(255),
  estado_regist_tapas_concre          VARCHAR(255),
  cambio_repara_tapas_concre          VARCHAR(255),
  debe_echars_agua_thorge_confir      VARCHAR(255),  -- ¿Se echó agua?
  debe_echars_agua_thorge_confir_1    VARCHAR(255),  -- ¿Se echó Thorgel? (renombrado)
  lectur_resist_despue_echar          VARCHAR(255),
  observacion                         VARCHAR(255)
);

