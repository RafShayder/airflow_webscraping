
CREATE TABLE IF NOT EXISTS raw.ge_info_general_de_tanque (
    task_id                       VARCHAR(255) NOT NULL,
    site_id                       VARCHAR(255) NOT NULL,
    sub_wo_id                     VARCHAR(255) NOT NULL,
    codigo_unico_de_tanque        VARCHAR(255),
    tipo_de_tanque                VARCHAR(255),
    capacidad_gln                 VARCHAR(255),
    nivel_de_combustible_gln      VARCHAR(255),
    autono_combus_actual_hrs      VARCHAR(255),
    hay_prefil_combus_tanque      VARCHAR(255),
    fecha_ultimo_cambio_prefil    VARCHAR(255),
    cambiar_pre_filtro            VARCHAR(255),
    limpia_lijar_pintar_corros    VARCHAR(255),
    tanque_etiquetado_cod_unico   VARCHAR(255),
    observacion                   VARCHAR(255)
);
