# ============================================================================
# Configuración de Entorno - Energy Facilities
# ============================================================================
# Copia este archivo a .env y completa con tus valores reales
# cp .env.example .env
# ============================================================================

# ----------------------------------------------------------------------------
# Entorno y Logging
# ----------------------------------------------------------------------------
ENV_MODE=dev
LOGGING_LEVEL=INFO

# ----------------------------------------------------------------------------
# PostgreSQL - Desarrollo
# ----------------------------------------------------------------------------
POSTGRES_USER=tu_usuario_postgres
POSTGRES_PASS=tu_contraseña_postgres
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=tu_base_de_datos

# ----------------------------------------------------------------------------
# PostgreSQL - Producción
# ----------------------------------------------------------------------------
POSTGRES_USER_PROD=tu_usuario_postgres_prod
POSTGRES_PASS_PRD=tu_contraseña_postgres_prod
POSTGRES_HOST_PROD=tu_host_postgres_prod
POSTGRES_PORT_PROD=5432
POSTGRES_DB_PROD=tu_base_de_datos_prod

# ----------------------------------------------------------------------------
# SFTP - Energía
# ----------------------------------------------------------------------------
SFTP_HOST_ENERGIA=tu_host_sftp_energia
SFTP_USER_ENERGIA=tu_usuario_sftp_energia
SFTP_PASS_ENERGIA=tu_contraseña_sftp_energia

# ----------------------------------------------------------------------------
# SFTP - DAAS
# ----------------------------------------------------------------------------
SFTP_HOST_DAAS=tu_host_sftp_daas
SFTP_USER_DAAS=tu_usuario_sftp_daas
SFTP_PASS_DAAS=tu_contraseña_sftp_daas

# ----------------------------------------------------------------------------
# SFTP - DAAS Producción
# ----------------------------------------------------------------------------
SFTP_HOST_DAAS_PROD=tu_host_sftp_daas_prod

# ----------------------------------------------------------------------------
# Teleows / Integratel Portal
# ----------------------------------------------------------------------------
TELEOWS_USERNAME=tu_usuario_teleows
TELEOWS_PASSWORD=tu_contraseña_teleows

# ----------------------------------------------------------------------------
# Proxy (opcional)
# ----------------------------------------------------------------------------
# Formato: host:puerto (ej: telefonica01.gp.inet:8080)
PROXY=

# ----------------------------------------------------------------------------
# Variables adicionales de Teleows (opcionales)
# Estas pueden sobrescribir valores del YAML si se necesitan
# ----------------------------------------------------------------------------
TELEOWS_DOWNLOAD_PATH=tmp/teleows
TELEOWS_MAX_IFRAME_ATTEMPTS=60
TELEOWS_MAX_STATUS_ATTEMPTS=60
TELEOWS_OPTIONS_TO_SELECT=CM,OPM
TELEOWS_DATE_MODE=2
TELEOWS_DATE_FROM=2025-09-01
TELEOWS_DATE_TO=2025-09-10
TELEOWS_EXPORT_OVERWRITE_FILES=true
TELEOWS_PROXY=
TELEOWS_HEADLESS=true
TELEOWS_GDE_OUTPUT_FILENAME=Console_GDE_export.xlsx
TELEOWS_DYNAMIC_CHECKLIST_OUTPUT_FILENAME=DynamicChecklist_SubPM.xlsx
